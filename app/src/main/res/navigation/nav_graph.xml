<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/firstFragment">

    <fragment
        android:id="@+id/firstFragment"
        android:name="edu.uw.tcss450.sprint0.ui.FirstFragment"
        android:label="fragment_first"
        tools:layout="@layout/fragment_first" >
        <action
            android:id="@+id/action_firstFragment_to_colorFragment"
            app:destination="@id/colorFragment" />
    </fragment>
    <fragment
        android:id="@+id/colorFragment"
        android:name="edu.uw.tcss450.sprint0.ui.ColorFragment"
        android:label="fragment_color"
        tools:layout="@layout/fragment_color" >
        <argument
            android:name="color"
            app:argType="integer" />
    </fragment>
</navigation>